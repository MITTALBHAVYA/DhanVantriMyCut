<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PlantImageRecognitionUsingML" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

    </style>

    <!--
Font family: Inter
Line height: 22sp
(identical to box height)
-->

    <!-- styles.xml -->
    <style name="dhanvantri">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#053D00</item>
    </style>

    <!-- strings.xml -->
    <string name="dhanvantri">
        DhanVantri
    </string>

    <style name="Theme.PlantImageRecognitionUsingML.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>